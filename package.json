    "@angular/core": "~11.2.4",
    "@angular/forms": "~11.2.4",
    "@angular/platform-browser": "~11.2.4",
    "@angular/platform-browser-dynamic": "~11.2.4",
    "@angular/router": "~11.2.4",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "animate.css": "^4.1.1",
    "chart.js": "^2.5.0",
    "easy-pie-chart": "2.1.x",
    "hammerjs": "^2.0.8",
    "mdb-calendar": "file:mdb-calendar-11.1.0.tgz",
    "mdb-color-picker": "file:mdb-color-picker-11.1.0.tgz",
    "mdb-draggable": "file:mdb-draggable-11.1.0.tgz",
    "mdb-file-upload": "file:mdb-file-upload-11.1.0.tgz",
    "mdb-filter": "file:mdb-filter-11.1.0.tgz",
    "mdb-sortable": "file:mdb-sortable-11.1.0.tgz",
    "mdb-table-editor": "file:mdb-table-editor-11.1.0.tgz",
    "mdb-wysiwyg": "file:mdb-wysiwyg-11.1.0.tgz",
    "ng-uikit-pro-standard": "file:ng-uikit-pro-standard-11.1.0.tgz",
    "ngx-useful-swiper": "^10.0.1",
    "rxjs": "~6.5.4",
